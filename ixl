javascript:
function updateStatistics() {
    const QrandomNumber = Math.floor(Math.random() * (250 - 40 + 1)) + 40;
    const SSrandomNumber = Math.floor(Math.random() * (99 - 95 + 1)) + 95;
    const SECrandomNumber = Math.floor(Math.random() * (59 - 0 + 1));
    const normalizedQ = (QrandomNumber - 40) / (250 - 40);
    const minTime = 10;
    const maxTime = 59;
    const time = Math.floor(normalizedQ * (maxTime - minTime + 1)) + minTime;

    document.querySelectorAll('.statistic-container.problems-attempted').forEach(container => {
        container.innerHTML = `
        <section class="statistic-container problems-attempted">
          <h2 class="statistic-header">
            <span class="statistic-text">Questions<br>answered</span>
            <span class="statistic-text short">Questions</span>
          </h2>
          <section class="statistic-content">${QrandomNumber}</section>
        </section>
        `;
    });

    document.querySelectorAll(`.statistic-container.time-elapsed`).forEach(container => {
        container.innerHTML = `
        <section class="statistic-container time-elapsed">
          <h2 class="statistic-header"><span class="statistic-text">Time<br>elapsed</span><span
              class="statistic-text short">Time</span></h2>
          <section class="statistic-content">
            <section class="practice-timer">
              <table class="timer">
                <tbody>
                  <tr class="time-values">
                    <td headers="hours-label" class="hours-value">00</td>
                    <td headers="minutes-label" class="minutes-value">${time}</td>
                    <td headers="seconds-label" class="seconds-value">${SECrandomNumber}</td>
                  </tr>
                  <tr class="time-labels">
                    <th id="hours-label" class="hours-label">hr</th>
                    <th id="minutes-label" class="minutes-label">min</th>
                    <th id="seconds-label" class="seconds-label">sec</th>
                  </tr>
                </tbody>
              </table>
            </section>
          </section>
        </section>
        `;
    });

    document.querySelectorAll('.statistic-container.smartscore').forEach(container => {
        container.innerHTML = `
        <section class="statistic-container smartscore">
          <h2 class="statistic-header"><span class="statistic-text">SmartScore</span>
            <section class="statistic-subheader smartscore">
              <h3 class="subheader-text">out of 100</h3><img class="question-mark-icon"
                src="data:image/png;base64,"
                alt="">
              <section class="statistic-tool-tip-container">
                <p class="tool-tip">IXL's SmartScore is a dynamic measure of progress towards mastery, rather than a percentage
                  grade. It tracks your skill level as you tackle progressively more difficult questions. Consistently answer
                  questions correctly to reach excellence (90), or conquer the Challenge Zone to achieve mastery (100)! <a
                    href="/resources/IXL_SmartScore_guide.pdf" target="_blank">Learn more</a></p>
              </section>
            </section>
          </h2>
          <section class="statistic-content">
            <section class="smartscore-container"><span class="current-smartscore grow-smartscore challenge-zone"
                data-cy="current-smartscore" style="display: block;">${SSrandomNumber}</span></section>
            <section class="ribbons"><span class="ribbon red-ribbon"></span><span class="ribbon blue-ribbon"></span><img
                class="ribbon green-ribbon animated-ribbon"
                src="data:image/gif;base64,"
                alt=""></section>
            <section class="medals"></section>
          </section>
          <section class="break-link"><a href="/recommendations/">Need a break?</a></section>
        </section>
        `;
    });
}

updateStatistics();
